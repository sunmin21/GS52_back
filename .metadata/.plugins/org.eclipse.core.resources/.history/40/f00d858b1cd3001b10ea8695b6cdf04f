<mapper namespace="Holiday">
	<select id="selectHoliday" parameterType="map"
		resultType="com.gs52.vo.manager.HolidayVO">
		SELECT * FROM HOLIDAY
	</select>
	
	<select id="AddHoliday" parameterType="map"
		resultType="com.gs52.vo.manager.HolidayVO">
		INSERT INTO HOLIDAY(HOLIDAY_TITLE, HOLIDAY_DATE, HOLIDAY_ANNUAL_REPEAT) 
		VALUES(#{HOLIDAY_TITLE}, #{HOLIDAY_DATE}, #{HOLIDAY_ANNUAL_REPEAT});
	</select>
</mapper>