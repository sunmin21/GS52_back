<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper 
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Annual">
   	 <select id="selectAnnual" parameterType="int" resultType="com.gs52.vo.attendance.AnnualManageVO">
      SELECT * from VACATION where vacation_emp_id = #{userid}
   </select>
   	 <insert id="insertAnnual" parameterType="com.gs52.vo.attendance.AnnualManageVO">
   	 INSERT INTO VACATION(VACATION_INDEX,VACATION_EMP_ID,VACATION_DATE,VACATION_ATTEND_INFO_INDEX,VACATION_CONTENTS)
   	 VALUES(#{VACATION_INDEX},#{VACATION_EMP_ID},#{VACATION_DATE},#{VACATION_ATTEND_INFO_INDEX},#{VACATION_CONTENTS})
   	  
   	 </insert>
</mapper>